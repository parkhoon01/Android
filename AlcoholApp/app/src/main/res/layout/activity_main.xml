<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DA000000">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Home"
        android:textAlignment="center"
        android:textColor="#6706AC"
        android:textSize="34sp"
        android:textStyle="bold" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.gridlayout.widget.GridLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="50dp">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="190dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:scaleType="centerCrop"
                    app:layout_column="0"
                    app:layout_row="0"
                    app:srcCompat="@drawable/bod" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="210dp"
                    android:text="양주"
                    android:textColor="#D30AE1"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_column="0"
                    app:layout_row="0" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="190dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/tradition" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="210dp"
                    android:text="전통주"
                    android:textColor="#D30AE1"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_column="1"
                    app:layout_row="0" />

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="190dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="40dp"
                    android:scaleType="centerCrop"
                    app:layout_column="0"
                    app:layout_row="1"
                    app:srcCompat="@drawable/beer" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="240dp"
                    android:text="맥주"
                    android:textColor="#D30AE1"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_column="0"
                    app:layout_row="1" />

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="190dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="40dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/soju" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="240dp"
                    android:text="소주"
                    android:textColor="#D30AE1"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_column="1"
                    app:layout_row="1" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="190dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="40dp"
                    android:scaleType="centerCrop"
                    app:layout_column="0"
                    app:layout_row="3"
                    app:srcCompat="@drawable/mix" />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="240dp"
                    android:text="섞어먹는 술"
                    android:textColor="#D30AE1"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_column="0"
                    app:layout_row="3" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="190dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="40dp"
                    android:scaleType="centerCrop"
                    app:layout_column="1"
                    app:layout_row="3"
                    app:srcCompat="@drawable/food" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="240dp"
                    android:text="맛있는 술집"
                    android:textColor="#D30AE1"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_column="1"
                    app:layout_row="3" />
            </androidx.gridlayout.widget.GridLayout>
        </LinearLayout>
    </ScrollView>

</FrameLayout>